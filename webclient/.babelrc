{
    "presets": [
        ["@babel/preset-env", {"modules": false}],
        ["@babel/preset-typescript", {"modules": false}],
        ["@babel/preset-react", {"modules": false}]
    ],
    "plugins": [
        "@babel/proposal-class-properties",
        "@babel/proposal-object-rest-spread",
        [
            "module-resolver",
            {
                "alias": {
                    "^react-native$": "react-native-web"
                }
            }
        ]
    ]
}
